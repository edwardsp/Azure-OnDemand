# Wait for the Theia server to start
echo "Waiting for Theia server to open port ${port}..."
if wait_until_port_used "${host}:${port}" 600; then
  echo "Discovered code-server listening on port ${port}!"
else
  echo "Timed out waiting for Theia: server to open port ${port}!"
  clean_up 1
fi
sleep 2
